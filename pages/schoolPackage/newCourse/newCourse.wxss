.course_box{
  margin-bottom: 120rpx;
}
.newCourse{
  width: 100%;
  font-size: 30rpx;
  background: #fafafa;
}
.banner{
  width: 100%;
  height: 300rpx;
  background: #faa;
}
.slide-image{
  width: 100%;
  height: 300rpx;
}
.course_time{
  padding: 30rpx 0;
  color: #434343;
}
.icon{
  width: 32rpx;
  height: 32rpx;
  margin-left: 10rpx;
}
.blcok{
  background: #fafafa;
}
.blcok:last-child{
  padding-bottom: 30rpx;
}
.blcok_padding{
