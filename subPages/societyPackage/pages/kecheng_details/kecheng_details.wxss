.kecheng{
	width: 100%;
	background-color: #fafafa;
}
.kecheng_banner{
	width: 100%;
	height: 518rpx;
	position: relative;
}
.kecheng_banner image{
	width: 100%;
	height: 100%;
}
.kecheng_tip{
	width: 100%;
	position: absolute;
	bottom: 30rpx;
}
.kecheng_title{
	font-size: 38rpx;
	color: white;
	padding-left: 30rpx;
	line-height: 60rpx;
}
.label{
	width: 94%;
	margin: 0rpx auto;
}
.label .label_item_middle{
	display: inline-block;
	font-size: 20rpx;
	height: 33rpx;
	line-height: 33rpx;
	padding: 0rpx 12rpx;
	color: white;
	border: 1px solid #EAEAEA;
	border-radius: 30rpx;
	margin-right: 10rpx;
}
.kecheng_teacher{
	width: 100%;
	padding: 0 4%;
	box-sizing: border-box;
	background-color: white;
	border-bottom: 1px solid #f4f4f4;
	padding-bottom: 37rpx;
}
.teacher_title{
	font-size: 33rpx;
	color: black;
	font-weight: bold;
	margin: 33rpx 0 27rpx 0;
}
.teacher_text{
	font-size: 26rpx;
	color: #3a3a3a;
	line-height: 50rpx;
}

/*列表*/
.searchOver{
	width: 94%;
	height: 120rpx;
	margin: 0 auto;
	line-height: 120rpx;
	background-color: white;
}
.over_title{
	width: 100%;
	box-sizing: border-box;
	margin-top: 20rpx;
	border-bottom: 1px solid #f5f5f5;
}
.title_l{
	font-size: 30rpx;
	font-weight: bold;
}
.title_r{
	font-size: 26rpx;
	float: right;
	margin-right: 14rpx;
}
.kecheng_list{
	width: 92%;
	margin: 0 auto;
	padding: 0 14rpx;
	box-sizing: border-box;
}
.kecheng_item{
	width: 100%;
	background-color: white;
	border-bottom: 1px solid #f5f5f5;
	overflow: hidden;
}
.kecheng_item image{
	float: left;
	width: 22rpx;
	height: 24rpx;
	display: inline-block;
	transform: translateY(30rpx);
	margin-right: 20rpx;
}
.kecheng_name{
	font-size: 26rpx;
	color: #474747;
	line-height: 85rpx;
	float: left;
	font-weight: bold;
}
.kecheng_time{
	font-size: 20rpx;
	color: #474747;
	line-height: 85rpx;
	float: right;
}