.setting{
	width: 100%;
	min-height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background-color: #fafafa;
	border-top: 1px solid #ececec;
}
.public_item{
	width: 100%;
	padding: 0 32rpx;
	border-bottom: 1px solid #ececec;
	background-color: white;
	box-sizing: border-box;
	position: relative;
}
.item_text{
	width: 100%;
	height: 108rpx;
	line-height: 108rpx;
	/*font-size: 30rpx;*/
}
.arrow{
	width: 14rpx;
	height: 24rpx;
	position: absolute;
	top: 42rpx;
	right: 32rpx;
}
.marginTop22{
	margin-top: 22rpx;
}
.item_out{
	/*font-size: 30rpx;*/
	color: #fe5430;
	text-align: center;
	line-height: 108rpx;
	font-weight: bolder;
}