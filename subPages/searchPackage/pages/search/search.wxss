.search{
	width: 100%;
	min-height: 100%;
	position: absolute;
	top: 0;
	left: 0;
	background-color: rgb(250,250,250);
	z-index: 1;
}



/*搜索前*/
.section{
	width: 100%;
	overflow: hidden;
	padding-bottom: 30rpx;
	border-bottom: 1px solid rgb(234,234,234);
	position: fixed;
	top: 0;
	left: 0;
	background-color: white;
	z-index: 30;
}
.searchInput{
	width: 73%;
	height: 70rpx;
	float: left;
	margin-left: 30rpx;
}
.searchInput input{
	width: 100%;
	box-sizing: border-box;
	height: 70rpx;
	background-color: rgb(238,238,238);
	font-size: 30rpx;
	/*border-radius: 3px;*/
	border-top-left-radius:3px;
	border-bottom-left-radius:3px;
	padding-left: 55rpx;
	color: rgb(149,149,149);
	position: relative;
	z-index: 10;
}
.searchIcon{
	position: absolute;
	top: 18rpx;
	left: 40rpx;
	z-index: 20;
}
.clearBox{
	width: 60rpx;
	height: 70rpx;
	/*position: absolute;
	top: 0;
	right: 130rpx;*/
	background-color: rgb(238,238,238);
	z-index: 30;
	display: inline-block;
	position: relative;
	border-top-right-radius:3px;
	border-bottom-right-radius:3px;
}
.clear{
	position: absolute;
	top: 18rpx;
	right: 10rpx;
	z-index: 40;
}
.section .quxiao{
	font-size: 30rpx;
	margin-right: 30rpx;
	float: right;
	height: 70rpx;
	line-height: 70rpx;
	color: rgb(80,80,80);
}
/*搜索历史*/
.history{
	width: 100%;
}
.history_item{
	width: 100%;
	height: 109rpx;
	line-height: 109rpx;
	border-bottom: 1px solid rgb(232,232,232);
	font-size: 30rpx;
	padding: 0 30rpx 0 30rpx;
	color: rgb(70,70,70);
	box-sizing: border-box;
	position: relative;
	z-index: 10;
}
.allSearch{
	width: 100%;
	font-size: 30rpx;
	padding: 30rpx 0 30rpx 0;
	text-align: center;
	color: rgb(139,139,139);
	border-bottom: 1px solid rgb(232,232,232);
}

/*热门搜索*/
.hot_search{
	width: 100%;
	padding: 0 30rpx 0;
	font-size: 26rpx;
	color: #8f8f8f;
	padding-top: 40rpx;
	background-color: rgb(250,250,250);
	box-sizing: border-box;
}
.label{
	width: (100% - 20rpx);
	margin-top: 30rpx;
	overflow: hidden;
}
.label .label_item_middle{
	display: block;
	float: left;
	font-size: 26rpx;
	background-color: #eeeeee;
	padding: 10rpx 20rpx;
	color: #3c3c3c;
	border: 1px solid #EAEAEA;
	border-radius: 24px;
	margin: 15rpx 15rpx 15rpx 0;
}



/*搜索后*/
/*课程*/
.searchOver{
	width: 100%;
	height: 109rpx;
	line-height: 109rpx;
	background-color: white;
}
.over_title{
	width: 100%;
	height: 109rpx;
	padding: 0 30rpx 0;
	box-sizing: border-box;
}
.title_l{
	font-size: 26rpx;
	font-weight: bold;
}
.title_r{
	font-size: 26rpx;
	float: right;
}
.list{
	width: 100%;
	overflow: hidden;
	border-top: 1px solid rgb(231,231,231);
	border-bottom: 1px solid rgb(231,231,231);
	background-color: white;
}
.list_title{
	height: 50rpx;
	line-height: 50rpx;
}
.list_title_l{
	font-size: 36rpx;
	font-weight: bold;
}
.list_title_r{
	font-size: 28rpx;
	float: right;
	margin-top: 8rpx;
	color: #999999;
}
.list_box{
	width: 100%;
	padding-left: 30rpx;
	background-color: white;
	box-sizing: border-box;
}
.list_content{
	width: 100%;
	border-bottom: 1px solid #EAEAEA;
}
.list_item{
	width: 100%;
	position: relative;
	overflow: hidden;
}
.list_item_userhead{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	float: left;
	display: block;
	margin: 30rpx 30rpx 30rpx 0;
	background-color: #b5b5b5;
}
.list_item_middle{
	width: 85%;
	margin-top: 30rpx;
}
.list_item_middle_title{
	font-size: 28rpx;
	height: 69rpx;
	line-height: 30rpx;
	font-weight: bold;
	color: #666666;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.list_item_middle_tip{
	font-size: 24rpx;
	color: #999999;
	height: 45rpx;
	line-height: 45rpx;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}
.list_item_arrow{
	width: 15rpx;
	height: 30rpx;
	position: absolute;
	top: 65rpx;
	right: 30rpx;
}
/*搜索后*/
/*活动*/
.top_margin{
	margin-top: 30rpx;
}
.panal{
	width: 100%;
	color: #434343;
	padding: 40rpx 30rpx;
	box-sizing: border-box;
	font-size: 30rpx;
	border-top: 1px solid #eeeeee;
	border-bottom: 1px solid #eeeeee;
	background-color: white;
}
.panal image{
	width: 100%;
	height: 400rpx;
	margin-top: 40rpx;
	border-radius: 5px;
	background-color: #b5b5b5;
}

/*搜索后*/
/*文章*/
.writings_list{
	width: 100%;
	padding: 0 30rpx;
	border-top: 1px solid #eeeeee;
	border-bottom: 1px solid #eeeeee;
	box-sizing: border-box;
	background-color: white;
	margin-bottom: 40rpx;
}
.writings_item{
	width: 100%;
	box-sizing: border;
	border-bottom: 1px solid #eeeeee;
	overflow: hidden;
	padding: 30rpx 0;
	position: relative;
}
.writings_text_l{
	/*width: 68%;*/
	font-size: 28rpx;
	color: #373737;
	float: left;
}
.min_width{
	width: 68%;
}
.writings_user{
	display: block;
	font-size: 24rpx;
	margin-top: 30rpx;
	color: #8e8e8e;
	position: absolute;
	left: 0;
	bottom: 30rpx;
}
.writings_user_head{
	width: 30rpx;
	height: 30rpx;
	border-radius: 50%;
	display: inline-block;
	margin: 0rpx;
	transform: translateY(5rpx);
}
.writings_text_r{
	float: right;
}
.writings_text_r image{
	width: 200rpx;
	height: 150rpx;
}


/*课程图标*/
.kclabel{
	width: 30rpx;
	height: 26rpx;
	transform: translateY(5rpx);
	margin-right: 15rpx;
}
/*活动图标*/
.hdlabel{
	width: 34rpx;
	height: 26rpx;
	transform: translateY(5rpx);
	margin-right: 15rpx;
}
/*文章图标*/
.wzlabel{
	width: 27rpx;
	height: 27rpx;
	transform: translateY(5rpx);
	margin-right: 15rpx;
}
/*搜索展开项图标*/
/*时间*/
.shijian{
	width: 31rpx;
	height: 31rpx;
	transform: translateY(3rpx);
	margin-right: 20rpx;
}
/*关闭*/
.cha{
	width: 26rpx;
	height: 26rpx;
	position: absolute;
	right: 30rpx;
	top: 42rpx;
	z-index: 20;
}



.no_image{
	height: 120rpx;
}
.no_title_shut{
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.no_border{
	border-bottom: none;
}
.no_searchData{
	font-size: 30rpx;
	width: 100%;
	text-align: center;
	margin: 400rpx auto 30rpx;
	color: #a1a3a6;
}
.no_searchData image{
	width: 200rpx;
	height: 200rpx;
	margin-bottom: 80rpx;
}